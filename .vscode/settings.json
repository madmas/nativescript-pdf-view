{
	"files.exclude": {
		"**/*.js": {
			"when": "$(basename).ts"
		},
		"**/*.js.map": {
			"when": "$(basename)"
		}
	},
	"NativescriptExtend.fileAssociations": [
	],
	"files.exclude": {
		"schema": true
	},
	"NativescriptExtend.format.enabled": true,
	"NativescriptExtend.validation.enabled": true
}
